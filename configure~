then :

         ze_happy="no"

else $as_nop

         save_CPPFLAGS="$CPPFLAGS"
         save_LDFLAGS="$LDFLAGS"
         save_LIBS="$LIBS"

         ZE_CPPFLAGS=""
         ZE_LDFLAGS=""
