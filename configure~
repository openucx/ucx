    HIP_CPPFLAGS="-D__HIP_PLATFORM_HCC__ -D__HIP_PLATFORM_AMD__ -I$with_rocm/include/hip -I$with_rocm/include"
    HIP_LDFLAGS="-L$with_rocm/hip/lib -L$with_rocm/lib"
