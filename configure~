#(
y|Y|yes|Yes|YES)
    DX_FLAG_pdf=1


test "$DX_FLAG_doc" = "1" \
|| as_fn_error $? "doxygen-pdf requires doxygen-pdf" "$LINENO" 5

;; #(
n|N|no|No|NO)
    DX_FLAG_pdf=0

;; #(
*)
    as_fn_error $? "invalid value '$enableval' given to doxygen-pdf" "$LINENO" 5
;;
esac

else $as_nop

DX_FLAG_pdf=1


test "$DX_FLAG_doc" = "1" || DX_FLAG_pdf=0



