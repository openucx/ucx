         if test "x$ze_happy" = "xyes"
then :
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for zeInit in -lze_loader" >&5
printf %s "checking for zeInit in -lze_loader... " >&6; }
if test ${ac_cv_lib_ze_loader_zeInit+y}
