        # DDP support
        if test "x$have_mlx5" = xyes
then :

           ac_fn_check_decl "$LINENO" "MLX5DV_CONTEXT_MASK_OOO_RECV_WRS" "ac_cv_have_decl_MLX5DV_CONTEXT_MASK_OOO_RECV_WRS" "#include <infiniband/mlx5dv.h>
