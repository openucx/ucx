fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_amdhip64_hipFree" >&5
printf "%s\n" "$ac_cv_lib_amdhip64_hipFree" >&6; }
if test "x$ac_cv_lib_amdhip64_hipFree" = xyes
then :
  hip_happy=yes
else $as_nop
  hip_happy=no
fi

fi
    if test "x$hip_happy" = xyes
then :
  HIP_CXXFLAGS="--std=gnu++11"
fi

