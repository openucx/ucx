then :

    UCM_MODULE_LDFLAGS=$UCM_MODULE_LDFLAGS_TEST

    ucm_ldflags_happy=yes

else $as_nop

    ucm_ldflags_happy=no

fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

